@using Sistema.Controllers
@{
    #region Layout
    Layout = "~/Views/Shared/_Layout.cshtml";
    #endregion

    #region ViewBags
    var traducaoHelper = (Core.Helpers.TraducaoHelper)ViewBag.TraducaoHelper;

    List<Core.Entities.Saque> contaRentabilidade = ViewBag.SaquesContaRentabilidade;
    List<Core.Entities.Saque> contaBonus = ViewBag.SaquesContaBonus;
    List<Core.Entities.Saque> contaInvest = ViewBag.SaquesContaInvest;

    var SolicitarSaqueRentabilidade = ViewBag.SolicitarSaqueRentabilidade;
    var SolicitarSaqueBonus = ViewBag.SolicitarSaqueBonus;
    var SolicitarSaqueInvest = ViewBag.SolicitarSaqueInvest;

    ViewBag.Title = traducaoHelper["SAQUES"].ToString().ToLower();
    #endregion

    #region Variaveis
    var moedaPadrao = Core.Helpers.ConfiguracaoHelper.GetMoedaPadrao();
    String conta = "";
    if (ViewBag.ContaDeposito == "BTC")
    {
        conta = traducaoHelper["CONTA"] + " BTC: " + ViewBag.ContaBTC;
    }
    else if (ViewBag.ContaDeposito == "LTC")
    {
        conta = traducaoHelper["CONTA"] + " LTC: " + ViewBag.ContaLTC;
    }
    else if (ViewBag.ContaDeposito == "USDT")
    {
        conta = traducaoHelper["CONTA"] + " USDT: " + ViewBag.ContaUSDT;
    }
    var msgSaque = traducaoHelper["SOLICITACAO_SAQUE_CONFIRMACAO"] + @"\n\n" + conta;

    var saldoInvestimento = ViewBag.SaldoInvestimento;
    var msgRenovacao = traducaoHelper["SOLICITACAO_RENOVACAO_CONFIRMACAO"];
    var msgRenovacaoAutomatica = traducaoHelper["SOLICITACAO_RENOVACAO_AUTOMATICA_CONFIRMACAO"];
    var msgCancelarRenovacaoAutomatica = traducaoHelper["SOLICITACAO_CANCELAR_AUTOMATICA_CONFIRMACAO"];

    #endregion
}

@section pageStyles {
    @Styles.Render("~/Content/table")
    @Styles.Render("~/Content/sweetAlert")
}

@section head {}

@section pageScripts {
    @Scripts.Render("~/scripts/table")
    @Scripts.Render("~/scripts/dataPiker")
