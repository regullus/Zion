                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var saque in contaInvest.OrderByDescending(p => p.ID))
                            {
                                <tr>
                                    <td>
                                        @if (saque.StatusAtual.Status == Core.Entities.SaqueStatus.TodosStatus.Pago)
                                        {
                                            <a href="javascript:void(0)" onclick="DadosPago('@saque.StatusAtual.Mensagem')" class="gridDetail" title=@traducaoHelper["DETALHES"]><i class="fa fa-search"></i></a>
                                        }
                                    </td>
                                    <td class="text-center">@saque.StatusInicial.Data.ToString("dd/MM/yyyy")</td>
                                    <td class="text-center">@moedaPadrao.Simbolo @saque.Liquido.Value.ToString(@moedaPadrao.MascaraOut)</td>
                                    <td class="text-center">@moedaPadrao.Simbolo @saque.Taxas.Value.ToString(@moedaPadrao.MascaraOut)</td>
                                    <td class="text-center">@moedaPadrao.Simbolo @saque.Total.Value.ToString(@moedaPadrao.MascaraOut)</td>
                                    @{
                                        var label = "label-default";
                                        switch (saque.StatusAtual.Status)
                                        {
                                            case Core.Entities.SaqueStatus.TodosStatus.Processando:
                                            case Core.Entities.SaqueStatus.TodosStatus.Indefinido:
                                            case Core.Entities.SaqueStatus.TodosStatus.Solicitado:
                                                label = "label-warning";
                                                break;
                                            case Core.Entities.SaqueStatus.TodosStatus.Cancelado:
                                            case Core.Entities.SaqueStatus.TodosStatus.Estornado:
                                                label = "label-danger";
                                                break;
                                            case Core.Entities.SaqueStatus.TodosStatus.Efetuado:
                                            case Core.Entities.SaqueStatus.TodosStatus.Pago:
                                                label = "label-success";
                                                break;
                                        }
                                    }
                                    <td class="text-center">@(ViewBag.ContaDeposito == "BTC" ? "BTC: " + saque.Carteira : ViewBag.ContaDeposito == "LTC" ? "LTC: " + saque.Carteira : "")</td>
                                    <td class="text-center"><span class="label @(label)">@saque.StatusAtual.Status.ToString()</span></td>
                                    <td class="text-center">
                                        @if (saque.StatusAtual.Status == Core.Entities.SaqueStatus.TodosStatus.Solicitado || saque.StatusAtual.Status == Core.Entities.SaqueStatus.TodosStatus.Indefinido)
                                        {
                                            <a href="javascript:void(0);" onclick="Cancelar(@saque.ID)" class="btn btn-circle red" data-toggle="tootip" data-original-title="@traducaoHelper["CANCELAR_SOLICITACAO_SAQUE"]">@traducaoHelper["CANCELAR"]</a>
                                        }
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                }
            </div>
        </div>
    </div>

</div>

