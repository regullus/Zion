        var atualizar = true;

        function DadosPago(hash) {
            $('#dadosPago-hash').text(hash);
            $('#dadosPago-hash').attr('href', "https://www.blockchain.com/btc/tx/" + hash);
            $('#dadosPago').modal('show');
        }

        function Cancelar(id) {
            $('#cancelar').modal();
            $('#cancelar div.modal-footer a').attr('href', '@Url.Action("cancelar", "saque")/' + id);
        };

        function SacarRentabilidade(valor, senha, token) {
            $('#formularioRentabilidade').hide();
            $("#saqueRentabilidade").modal('show');
            $('#mensagemRentabilidade').show();
            $('#mensagemRentabilidade p').html('@traducaoHelper["AGUARDE"]...');

            $.post('@Url.Action("solicitar")', { valor: valor, senha: senha, token: token, tipo: '1'}, function (data) {
                if (data == 'OK') {
                    $('#mensagemRentabilidade p').html('@traducaoHelper["SAQUE_SOLICITADO_SUCESSO"]');
                    atualizar = true;
                } else {
                    $('#mensagemRentabilidade p').html(data);
                    atualizar = false;
                }
                $('#saqueRentabilidade div.modal-footer').show();
            });
        };

