        function TransferirRentabilidade() {
            var valor = $('#valorTrasferenciaRentabilidade').inputmask('unmaskedvalue');
            var login = $('#LoginUsuarioRentabilidade').val();
            var tokenTransferirRentabilidade = $("#token2FATransferirRentabilidade").val();
            $('#mensagemRentabilidade').show();
            $('#mensagemRentabilidade p').html('@traducaoHelper["AGUARDE"]...');
            if(valor != ''){
                $.post('@Url.Action("transferirRentabilidade")', { login: login, valor: valor, token: tokenTransferirRentabilidade }, function (data) {
                    if (data == 'OK') {
                       swal('@Html.Raw(traducaoHelper["SUCESSO"])', '@Html.Raw(traducaoHelper["TRANSFERENCIA_REALIZADA_SUCESSO"])', 'success');
                    } else {
                        swal('@Html.Raw(traducaoHelper["ALERTA"])', data, 'info');
                    }
                });
            }
        }

