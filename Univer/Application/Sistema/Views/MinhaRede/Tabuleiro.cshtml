    @if (ViewBag.Timer != null)
    {
        <script>
            //Contador de tempo para pagamento
            const second = 1000;
            const minute = second * 60;
            const hour = minute * 60;
            let count_down = new Date('@ViewBag.Timer').getTime();
            let x = setInterval(() => countDown(), second);
            function countDown() {
                let now = new Date(Date.now()).getTime();
                let diff = count_down - now;
                if (diff > 0) {
                    var min = Math.floor(diff % hour / minute).toString();
                    var seg = Math.floor(diff % minute / second).toString();
                    if (min.length == 1) {
                        min = '0' + min;
                    }
                    if (seg.length == 1) {
                        seg = '0' + seg;
                    }
                    document.getElementById('minutes').innerText = min;
                    document.getElementById('seconds').innerText = seg;

                    $("#minutes").removeClass('hidden');
                    $("#pontos").removeClass('hidden');
                    $("#seconds").removeClass('hidden');
                } else {
                    $("#minutes").addClass('hidden');
                    $("#pontos").addClass('hidden');
                    $("#seconds").addClass('hidden');
                }
            }
        </script>
