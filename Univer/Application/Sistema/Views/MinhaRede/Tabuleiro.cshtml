            </div>
            <div class="modal-body" style="background-color: #fff;">
                <div class="row">
                    <div class="col-md-12">
                        <h3 style="font-weight: 500;">@(traducaoHelper["NOME"])</h3>
                        <h3 style="padding-left: 10px;" id="infoNome"></h3>

                        <div class="panel-group accordion" id="accordion1">
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h4 class="panel-title">
                                        <a class="accordion-toggle accordion-toggle-styled" data-toggle="collapse" data-parent="#accordion1" href="#collapse_1"> @traducaoHelper["DADOS_PAGAMENTE_PIX"] </a>
                                    </h4>
                                </div>
                                <div id="collapse_1" class="panel-collapse in">
                                    <div class="panel-body" style="height:400px; overflow-y:auto; background-color: #eee;">
                                        <h3 style="font-weight: 500;">@(traducaoHelper["CELULAR"]) @(traducaoHelper["IDIOMA"] == "pt-BR" ? "(PIX)" : "")</h3>
                                        <h3 style="padding-left: 10px;" id="infoCelular"></h3>
                                        <img id="qrCodePIX" class="center" src="" />
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default panelBTC">
                                <div class="panel-heading">
                                    <h4 class="panel-title">
                                        <a class="accordion-toggle accordion-toggle-styled collapsed" data-toggle="collapse" data-parent="#accordion1" href="#collapse_2"> @traducaoHelper["DADOS_PAGAMENTE_BTC"] </a>
                                    </h4>
                                </div>
                                <div id="collapse_2" class="panel-collapse collapse">
