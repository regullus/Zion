        $('.btnTabuleiro').on('click', function () {
            var idTarget = $(this).attr("data-id");
            var idTabuleiro = $('#tabuleiro').val();
            var nivel = $(this).attr("data-name");
            if (idTarget == $('#Master').attr("data-id") || $('#Master').attr("data-id") == @idUsuario) {
                $.ajax({
                    url: "@Url.Action("GetDados")",
                    type: "POST",
                    data: {
                        'usuarioID': '@(idUsuario)',
                        'targetID': idTarget,
                        'tabuleiroID': idTabuleiro,
                        'nivel': nivel,
                        'token': '@(ViewBag.Token)'
                    },
                    success: function (data) {
                        if (data != null) {
                            $("#infoNome").html(data.Nome);
                            $("#infoCelular").html(data.Celular);
                            $("#qrCodePIX").attr('src', 'https://chart.googleapis.com/chart?chs=250x250&cht=qr&chl=' + data.Celular);
                            $("#infoTitulo").html('@(traducaoHelper["ALVO"])');

                            if (data.Carteira) {
                                $("#infoCripto").html(data.Carteira);
                                $("#carteiraHeader").show();
                                $("#infoCripto").show();
                                $("#qrCodeBTC").attr('src', 'https://chart.googleapis.com/chart?chs=250x250&cht=qr&chl=' + data.Carteira);
                            } else {
                                $("#carteiraHeader").html('@traducaoHelper["CARTEIRA_BTC_NAO_CADASTRADA"]');
                            }

                            if ($('#Master').attr("data-id") == @idUsuario) {
                                $(".panelBTC").hide();
                                $("#qrCodePIX").hide();
                            }

                            $('#modDados').modal('show');
                        } else {
                            Swal.fire({
                                title: '@traducaoHelper["ALERTA"]: ' + xhr.status + '!',
                                text: '@traducaoHelper["SEM_DADOS"]',
                                icon: 'warning',
                                confirmButtonText: 'OK'
                            });
                        }
                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        if (thrownError.toString() == '@traducaoHelper["TOKEN_INVALIDO"]') {
                            //Reload page
                            location.reload();
                        } else {
                            Swal.fire({
                                title: '@traducaoHelper["ERRO"]: ' + xhr.status + '!',
                                text: '@traducaoHelper["ERRO"]:' + thrownError.toString(),
                                icon: 'error',
                                confirmButtonText: 'OK'
                            });
                        }
                    },
                });
            }
        });

