@section pageScripts {
    @if (!ViewBag.RedeTabuleiro)
    {
        <script type="text/javascript">
            $(document).ready(function () {
                //$('.owl-carousel').owlCarousel({
                //    items: 1,
                //    loop: true,
                //    margin: 10,
                //    autoplay: true,
                //    autoplayTimeout: 5000,
                //    autoplayHoverPause: false,
                //    dots: true,
                //});

                if ('@ViewBag.BolqueioRenovacao' == 'true') {
                    blink("#bolqueioRenovacao");
                }

                @*if ('@usuario.DataMigracao.HasValue' == 'True'  && '@Core.Helpers.ConfiguracaoHelper.GetString("EXIGI_MIGRACAO")' == 'true' ) {*@
                if ('@usuarioContainer.PendenteMigracao' == 'True') {
                    blink("#migracao");
                }

                $(function () {
                  $('[data-toggle="tooltip"]').tooltip({
                    template: '<div class="tooltip md-tooltip" role="tooltip"><div class="tooltip-arrow md-arrow"></div><div class="tooltip-inner md-inner" style="font-size:medium; border-radius:5px!important;"></div></div>'
                  })
                })

                var btnCopiarCadastro = $("#cadastroLinkCopiar");
                var copiado = $("#copiado");
                var copiadoTemp = 0;
                copiado.hide();

                var btnCopiarLojaExterna = $("#lojaExternaLinkCopiar");
                var copiadoLojaExterna = $("#copiadoLojaExterna");
                var copiadoTempLojaExterna = 0;
                copiadoLojaExterna.hide();

                btnCopiarCadastro.on('click', function () {

                    var el = document.createElement('textarea');

                    el.value = '@(Core.Helpers.ConfiguracaoHelper.GetString("DOMINIO") + "p/" + Helpers.Local.nomeUsuario)';

                    el.setAttribute('readonly', '');
                    el.style = { position: 'absolute', left: '-9999px' };
                    document.body.appendChild(el);

                    el.select();

                    document.execCommand('copy');

                    copiado.fadeIn();
                    clearTimeout(copiadoTemp);
                    copiadoTemp = setTimeout(function () { copiado.fadeOut(); }, 1000);

                    document.body.removeChild(el);
                });

                btnCopiarLojaExterna.on('click', function () {

                    var el = document.createElement('textarea');

                    el.value = '@(Core.Helpers.ConfiguracaoHelper.GetString("URL_SITE_LOJA") + Helpers.Local.nomeUsuario)';

                    el.setAttribute('readonly', '');
                    el.style = { position: 'absolute', left: '-9999px' };
                    document.body.appendChild(el);

                    el.select();

                    document.execCommand('copy');

                    copiadoLojaExterna.fadeIn();
                    clearTimeout(copiadoTempLojaExterna);
                    copiadoTempLojaExterna = setTimeout(function () { copiadoLojaExterna.fadeOut(); }, 1000);

                    document.body.removeChild(el);
                });

            });
        </script>

        <script type="text/javascript">
            function blink(selector) {
                setTimeout(function () {
                    $(selector).fadeOut("slow", function () {
                        $(this).fadeIn("slow", function () {
                            blink(this); // recursive function will call after fadeIn finish
                        });
                    });

                }, 500);
            }
        </script>

    }
}

