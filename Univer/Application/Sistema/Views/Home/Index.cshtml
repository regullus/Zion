@{
    #region Layout

    Layout = "~/Views/Shared/_Layout.cshtml";

    #endregion

    #region init

    var traducaoHelper = (Core.Helpers.TraducaoHelper)ViewBag.TraducaoHelper;

    #endregion

    #region ViewBags

    ViewBag.Title = "Sistema";
    ViewBag.Description = "Sistema";
    ViewBag.Author = "";
    ViewBag.ReferrerMode = ReferrerMode.OriginWhenCrossOrigin;
    ViewBag.TwitterCard = new SummaryLargeImageTwitterCard("");
    ViewBag.Title = traducaoHelper["DOCUMENTOS"].ToString().ToLower();
    var usuarioContainer = (Sistema.Containers.UsuarioContainer)ViewBag.UsuarioContainer;
    var usuario = usuarioContainer.Usuario;
    Boolean blnExibeBanner = ViewBag.ExibeBanners ?? true;
    IList<Core.Entities.Arquivo> banners = ViewBag.Banners;
    IEnumerable<Core.Entities.Associacao> associacoes = null;
    IEnumerable<Core.Entities.Classificacao> classificacoes = null;
    IEnumerable<Core.Models.StoredProcedures.spC_AtivoMensal> ativoMensal = null;

    ViewBag.OpenGraph = new OpenGraphWebsite(ViewBag.Title, new OpenGraphImage(Url.AbsoluteContent("~/content/icons/open-graph-1200x630.png")) { Height = 630, Type = ContentType.Png, Width = 1200 })
    {
        Determiner = OpenGraphDeterminer.Blank,
        SiteName = Application.Name
    };

    List<SelectListItem> listaDerramamento = null;

    IEnumerable<Core.Models.TabuleiroNivelModel> tabuleiroConvites = null;
    IEnumerable<Core.Models.TabuleiroNivelModel> tabuleiroAtivos = null;
    IEnumerable<Core.Models.TabuleiroNivelModel> tabuleiroFinalizados = null;

    if (ViewBag.RedeTabuleiro)
    {
        #region Tabuleiro
        tabuleiroConvites = (IEnumerable<Core.Models.TabuleiroNivelModel>)ViewBag.TabuleirosConvite;
        tabuleiroAtivos = (IEnumerable<Core.Models.TabuleiroNivelModel>)ViewBag.TabuleirosAtivos;
        tabuleiroFinalizados = (IEnumerable<Core.Models.TabuleiroNivelModel>)ViewBag.TabuleirosFinalizados;
        #endregion
    }
    else
    {
        #region Nao Tabuleiro

        if (ViewBag.ExibeLadoDerramamento)
        {
            listaDerramamento = (List<SelectListItem>)ViewBag.ListaDerramamento;
        }

        associacoes = (IEnumerable<Core.Entities.Associacao>)ViewBag.Associacoes;
        classificacoes = (IEnumerable<Core.Entities.Classificacao>)ViewBag.Classificacoes;
        ativoMensal = (IEnumerable<Core.Models.StoredProcedures.spC_AtivoMensal>)ViewBag.AtivoMensal;

        #endregion
    }

    ViewBag.MascaraData = traducaoHelper["MASCARA_DATA"];
    if (ViewBag.MascaraData == null)
    {
        ViewBag.MascaraData = "dd/MM/yyyy";
    }

    #endregion

    #region Variaveis

    DateTime data;
    var binarioData = (IEnumerable<Core.Entities.Posicao>)ViewBag.BinarioData;
    var valorZerado = 0;

    var moedaPadrao = Core.Helpers.ConfiguracaoHelper.GetMoedaPadrao();

    var usuarioBinder = new Sistema.ModelBinders.UsuarioBinder();
    usuarioBinder.Usuario = usuario;
    var foto = usuarioBinder.Foto;

    if (String.IsNullOrEmpty(foto))
    {
        foto = Url.Content("~/Content/img/" + (usuario.Sexo == "M" ? Helpers.Local.Sistema + "/Homem" : Helpers.Local.Sistema + "/Mulher") + ".png");
    }
    else
    {
        foto = Url.Content(foto);
    }

    bool ExibeDataAtivacao = (usuario.DataAtivacao == null || usuario.DataAtivacao.Value == DateTime.Parse("1900-01-01 00:00:00.000")) ? false : true;
    bool BolqueioRenovacao = (ViewBag.BolqueioRenovacao == "true") ? true : false;


    var classificacao = classificacoes != null ? classificacoes.FirstOrDefault(c => c.Nivel == usuario.NivelClassificacao) : null;
    var usuarioAssociacao = associacoes != null ? associacoes.FirstOrDefault(a => a.Nivel == usuario.NivelAssociacao) : null;

    var acumuladoEsquerda = ViewBag.PontuacaoEsquerdo != null ? ViewBag.PontuacaoEsquerdo : 0;
    var acumuladoDireita = ViewBag.PontuacaoDireito != null ? ViewBag.PontuacaoDireito : 0;
    var acumuladoEsquerdaHoje = ViewBag.acumuladoEsquerdaHoje != null ? ViewBag.acumuladoEsquerdaHoje : 0;
    var acumuladoDireitaHoje = ViewBag.acumuladoDireitaHoje != null ? ViewBag.acumuladoDireitaHoje : 0;

    var pontosHojeE = String.Format("+{0} {1}", acumuladoEsquerdaHoje.ToString(moedaPadrao.MascaraOut), traducaoHelper["HOJE"]);
    var pontosHojeD = String.Format("+{0} {1}", acumuladoDireitaHoje.ToString(moedaPadrao.MascaraOut), traducaoHelper["HOJE"]);

    var pontos = ViewBag.Pontos == null ? 0 : ViewBag.Pontos;
    var pontosMax = ViewBag.PontosMax == null ? 0 : ViewBag.PontosMax;

    #endregion

}

