@model Core.Helpers.TraducaoHelper

<div id="resumo" class="modal fade openResumo" tabindex="-1" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">

        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">@Model["RESUMO_TITULO"]</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-6">
                        <label class="red">@Model["RESUMO_SALDO"]</label>
                    </div>
                    <div class="col-md-6">
                        <label class="total-saldo"></label>
                    </div>
                </div><div class="row">
                    <div class="col-md-6">
                        <label class="red">@Model["RESUMO_PAGAMENTO"]</label>
                    </div>
                    <div class="col-md-6">
                        <label class="total-pagamentos"></label>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" data-id="0" class="btn btn-sm green-seagreen ConfirmarAtivar" style="display:none">@Model["RESUMO_PAGAR"]</button>
            </div>
        </div>
    </div>
</div>

